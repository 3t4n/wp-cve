{
	"name": "studiopress/simple-urls",
	"type": "wordpress-plugin",
	"description": "Simple URLs is a complete URL management system that allows you create, manage, and track outbound links from your site by using custom post types and 301 redirects.",
	"homepage": "https://github.com/studiopress/simple-urls",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^5.3 || ^7",
		"sentry/sdk": "^3.2",
		"psr/container": "1.1.1",
		"http-interop/http-factory-guzzle": "1.1.1",
		"firebase/php-jwt": "^6.3"
	},
	"config": {
		"sort-order": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"phpcs": "phpcs --standard=WordPress --extensions=php -p -v ./includes/class-simple-urls.php ./includes/class-simple-urls-admin.php"
	},
	"support": {
		"issues": "https://github.com/studiopress/simple-urls/issues",
		"source": "https://github.com/studiopress/simple-urls"
	}
}
