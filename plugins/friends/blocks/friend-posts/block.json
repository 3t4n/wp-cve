{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "friends/friend-posts",
	"title": "Friend Posts",
	"category": "widgets",
	"textdomain": "friends",
	"icon": "groups",
	"attributes": {
		"author_inline": {
			"type": "boolean",
			"default": false
		},
		"author_name": {
			"type": "boolean",
			"default": true
		},
		"author_avatar": {
			"type": "boolean",
			"default": true
		},
		"show_date": {
			"type": "boolean",
			"default": true
		},
		"count": {
			"type": "number",
			"default": 5
		},
		"exclude_users": {
			"type": "string"
		},
		"only_users": {
			"type": "string"
		},
		"internal_link": {
			"type": "boolean",
			"default": false
		}
	},
	"example": {
		"attributes": {}
	},
	"editorScript": "file:./build/index.js"
}
