msgid "PayOp"
msgstr "PayOp"

# Настройка приема электронных платежей через PayOp.
msgid "Take payments via PayOp"
msgstr "Take payments via PayOp"

# Шлюз отключен
msgid "Gateway is disabled"
msgstr "Gateway is disabled"

# PayOp не поддерживает валюты Вашего магазина.
msgid "PayOp does not support the currency of your store"
msgstr "PayOp does not support the currency of your store"

# Активность способа оплаты
msgid "Enable PayOp payments"
msgstr "Enable PayOp payments"

# Активен
msgid "Enable/Disable"
msgstr "Enable/Disable"

# Название способа оплаты
msgid "Name of payment gateway"
msgstr "Name of payment gateway"

# Название способа оплаты, которое увидит пользователь при оформлении заказа
msgid "The name of the payment gateway that the user see when placing the order"
msgstr "The name of the payment gateway that the user see when placing the order"

# Публичный ключ
msgid "Public key"
msgstr "Public key"

# Секретный ключ
msgid "Secret key"
msgstr "Secret key"

# JWT Token
msgid "JWT Token"
msgstr "JWT Token"

# Требуется для Directpay! Выдается в личном кабинете https://payop.com/en/profile/settings/jwt-token
msgid "Required for Directpay! Issued in the client panel https://payop.com/en/profile/settings/jwt-token"
msgstr "Required for Directpay! Issued in the client panel https://payop.com/en/profile/settings/jwt-token"

# Выдается в Личном кабинете https://payop.com
msgid "Issued in the client panel https://payop.com"
msgstr "Issued in the client panel https://payop.com"

# Описание способа оплаты, которое клиент будет видеть на вашем сайте.
msgid "Description of the payment gateway that the client will see on your site"
msgstr "Description of the payment gateway that the client will see on your site"

# Автозавершение заказа
msgid "Order completion"
msgstr "Order completion"

# Автоматический перевод заказа в статус \"Выполнен\" после успешной оплаты
msgid "Automatic transfer of the order to the status \"Completed\" after successful payment"
msgstr "Automatic transfer of the order to the status \"Completed\" after successful payment"

# Язык платежной формы
msgid "Payment form language"
msgstr "Payment form language"

# Выберите язык платежной формы для Вашего магазина
msgid "Select the language of the payment form for your store"
msgstr "Select the language of the payment form for your store"

# Русский
msgid "Russian"
msgstr "Russian"

# Английский
msgid "English"
msgstr "English"

# Оплата заказа #
msgid "Payment order #"
msgstr "Payment order #"

# Запрос к платежному сервису был отправлен некорректно
msgid "Request to payment service was sent incorrectly"
msgstr "Request to payment service was sent incorrectly"

# Оплатить
msgid "Pay"
msgstr "Pay"

# Отказаться от оплаты & вернуться в корзину
msgid "Refuse payment & return to cart"
msgstr "Refuse payment & return to cart"

# Спасибо за Ваш заказ, пожалуйста, нажмите кнопку ниже, чтобы заплатить.
msgid "Thank you for your order, please click the button below to pay"
msgstr "Thank you for your order, please click the button below to pay"

# Платеж успешно оплачен
msgid "Payment successfully paid"
msgstr "Payment successfully paid"

# Платеж не оплачен
msgid "Payment not paid"
msgstr "Payment not paid"

# Платеж успешно завершен
msgid "Payment completed successfully"
msgstr "Payment completed successfully"
