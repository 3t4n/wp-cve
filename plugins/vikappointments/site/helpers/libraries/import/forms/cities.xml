<?xml version="1.0" encoding="utf-8"?>
<importable>

	<table name="#__vikappointments_cities" id="city">

		<column 
			name="city_name" 
			label="VAPMANAGECITY1"
			required="1"
		/>

		<column 
			name="city_2_code" 
			label="VAPMANAGECITY2"
			required="0"
			filter="word"
		/>

		<column 
			name="city_3_code" 
			label="VAPMANAGECITY3"
			required="0"
			filter="word"
		/>

		<column 
			name="latitude" 
			label="VAPMANAGECITY4"
			required="0"
		/>

		<column 
			name="longitude" 
			label="VAPMANAGECITY5"
			required="0"
		/>

		<column
			name="published"
			label="VAPMANAGECITY6"
			required="0"
			default="1"
			filter="uint"
		>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
		</column>

	</table>

	<cancel task="city.cancel" />

</importable>
