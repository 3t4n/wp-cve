<?xml version="1.0" encoding="utf-8"?>
<importable>

	<table name="#__vikappointments_option" id="option">

		<column 
			name="name" 
			label="VAPMANAGEOPTION2"
			required="1"
		/>
		
		<column 
			name="description" 
			label="VAPMANAGEOPTION3"
			required="0"
			filter="raw"
		/>

		<column 
			name="price" 
			label="VAPMANAGEOPTION4"
			required="0"
			default="0"
			filter="float"
			type="currency"
		/>

		<column 
			name="published" 
			label="VAPMANAGESERVICE6"
			required="0"
			default="1"
			filter="uint"
		>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
		</column>

		<column 
			name="single" 
			label="VAPMANAGEOPTION5"
			required="0"
			default="1"
			filter="uint"
		>
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</column>

		<column 
			name="maxq" 
			label="VAPMANAGEOPTION6"
			required="0"
			default="1"
			min="1"
			filter="uint"
		/>

		<column 
			name="required" 
			label="VAPMANAGEOPTION12"
			required="0"
			default="0"
			filter="uint"
		>
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</column>

		<column 
			name="id_group" 
			label="VAPMANAGESERVICE10"
			required="0"
			filter="uint"
			type="optiongroup"
		>
			<option value="-1">/</option>
			<option value="0">/</option>
		</column>

		<column 
			name="ordering" 
			label="VAPMANAGECUSTOMF6"
			required="0"
			filter="uint"
		/>

	</table>

	<cancel task="option.cancel" />

</importable>
