<?xml version="1.0" encoding="utf-8"?>
<importable>

	<table name="#__vikappointments_states" id="state">

		<column 
			name="state_name" 
			label="VAPMANAGESTATE1"
			required="1"
		/>

		<column 
			name="state_2_code" 
			label="VAPMANAGESTATE2"
			required="1"
			filter="word"
		/>

		<column 
			name="state_3_code" 
			label="VAPMANAGESTATE3"
			required="0"
			filter="word"
		/>

		<column
			name="published"
			label="VAPMANAGESTATE4"
			required="0"
			default="1"
			filter="uint"
		>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
		</column>

	</table>

	<cancel task="state.cancel" />

</importable>
