{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "blockons/video-slider",
  "title": "Video Slider",
  "textdomain": "blockons",
  "icon": "format-video",
  "category": "blockons-category",
  "keywords": [
    "video",
    "slider",
    "youtube",
    "vimeo",
    "blockons"
  ],
  "attributes": {
    "uniqueId": {
      "type": "string"
    },
    "alignment": {
      "type": "string",
      "default": "left"
    },
    "sliderSlides": {
      "type": "array",
      "default": [
        {
          "id": "video-one",
          "videoType": "youtube",
          "videoId": "",
          "coverImage": {},
          "customVideo": {}
        },
        {
          "id": "video-two",
          "videoType": "youtube",
          "videoId": "",
          "coverImage": {},
          "customVideo": {}
        },
        {
          "id": "video-three",
          "videoType": "youtube",
          "videoId": "",
          "coverImage": {},
          "customVideo": {}
        }
      ]
    },
    "sliderStyle": {
      "type": "string",
      "default": "one"
    },
    "sliderRoundNess": {
      "type": "string",
      "default": "square"
    },
    "sliderBorderWidth": {
      "type": "number",
      "default": 10
    },
    "sliderOuterRound": {
      "type": "number",
      "default": 4
    },
    "sliderBorderColor": {
      "type": "string",
      "default": "#000"
    },
    "transition": {
      "type": "string",
      "default": "slide"
    },
    "mode": {
      "type": "string",
      "default": "default"
    },
    "navigation": {
      "type": "boolean",
      "default": true
    },
    "navigationStyle": {
      "type": "string",
      "default": "default"
    },
    "navigationColor": {
      "type": "string",
      "default": "dark"
    },
    "navigationArrow": {
      "type": "string",
      "default": "ban"
    },
    "pagination": {
      "type": "boolean",
      "default": true
    },
    "paginationStyle": {
      "type": "string",
      "default": "bullets"
    },
    "paginationColor": {
      "type": "string",
      "default": "dark"
    },
    "showOnHover": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "blockons-slider-video",
  "style": [
    "file:./style-index.css",
    "blockons-fontawesome",
    "blockons-swiper-css"
  ]
}