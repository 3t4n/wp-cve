{
    "name": "aleksandr-guidrevitch/wp-meteor",
    "type": "wordpress-plugin",
    "description": "WP Meteor",
    "homepage": "https://wp-meteor.com/wp-meteor/",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Aleksandr Guidrevitch",
            "email": "alex@excitingstartup.com",
            "homepage": "https://wp-meteor.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "wpdesk\/wp-notice": "^3.2.1"
    },
    "autoload": {
        "psr-4": {
            "WP_Meteor\\Backend\\": "backend\/",
            "WP_Meteor\\Blocker\\": "blocker\/",
            "WP_Meteor\\Frontend\\": "frontend\/",
            "WP_Meteor\\Ajax\\": "ajax\/",
            "WP_Meteor\\Rest\\": "rest\/",
            "WP_Meteor\\Engine\\": "engine\/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
