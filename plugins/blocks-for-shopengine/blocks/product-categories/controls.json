{
    "shopengine_section_product_cats_start":{
        "type": "PanelBody",
        "label": "Categories"
    },
        "shopengine_product_cate_align":{
            "control": {
            "field": "SelectControl",
            "props": {
              "label": "Align",
                "options": [
                {
                    "value": "left",
                    "label": "Left"
                },
                {
                    "value": "center",
                    "label": "Center"
                },
                {
                    "value": "right",
                    "label": "Right"
                }
                ]
            }
            },
            "responsive": true,
            "type": "object",
            "default": {
               "desktop": "left"
            }
        },

    

    "shopengine_categories_label__section": {
      "control": {
          "field": "__experimentalHeading",
          "props": {
              "children" : "Categories Label"
          }
      },
      "type": "object"
      },
        "shopengine_product_cats_label_show":{
            "control": {
            "field": "ToggleControl",
            "props": {
              "label": "Show Label"
            }
            },
            "responsive": false,
            "type": "object",
            "default": {
              "desktop": true
            }
        },

        "shopengine_product_cate_label_color":{
          "control": {
          "field": "ColorPalette",
          "label": "Color",
          "props": {
            "clearable": true,
              "colors": [
              {
                  "name": "#000",
                  "color": "#000"
              },
              {
                  "name": "white",
                  "color": "#fff"
              },
              {
                  "name": "blue",
                  "color": "#3400F8"
              },
              {
                  "name": "#FE317D",
                  "color": "#FE317D"
              },
              {
                  "name": "#d423d4",
                  "color": "#d423d4"
              }
              ]
          }
          },
          "responsive": false,
          "type": "object",
          "default": {
          "desktop": "#000"
          }
      },
      "shopengine_product_cate_text_deco":{
          "control": {
          "field": "SelectControl",
          "props":{
            "label": "Text Decoration",
            "options": [
              {
                "value": "none",
                "label": "None"
              },
              {
                "value": "overline",
                "label": "Overline"
              },
              {
                "value": "underline",
                "label": "Underline"
              },
              {
                "value": "line-through",
                "label": "Line-through"
              }
            ]
          }
          },
          "responsive": false,
          "type": "object",
          "default": {
          "desktop": "none"
          }
      },
      "shopengine_label_cata_one_section_divider": {
         "control": {
             "field": "__experimentalDivider",
             "props": {
             }
         },
         "type": "object",
         "default": {
             "desktop": "Common Diverder"
         }
      },
      "shopengine_categories_link_label__section": {
         "control": {
             "field": "__experimentalHeading",
             "props": {
                 "children" : "Categories Links"
             }
         },
         "type": "object"
      },
      "tab_catagroy_start":{
         "type": "TabContainer",
         "label": "Choose Color"
       },
 
         "tab_normal_start":{
           "type": "TabControl",
           "label": "Normal"
         },
           "shopengine_catagroy_normal_color":{
             "control": {
               "field": "ColorPalette",
               "label": "Color",
               "props": {
                "clearable": true,
                 "colors": [
                   {
                       "name": "#A0A0A0",
                       "color": "#A0A0A0"
                   },
                   {
                       "name": "white",
                       "color": "#fff"
                   },
                   {
                       "name": "blue",
                       "color": "#3400F8"
                   },
                   {
                       "name": "#FE317D",
                       "color": "#FE317D"
                   },
                   {
                       "name": "#d423d4",
                       "color": "#d423d4"
                   }
                   ]
               }
             },
             "responsive": false,
             "type": "object",
             "default": {
               "desktop": "#A0A0A0"
             }
           },
         "tab_normal_end":{
           "type": "TabControl"
         },
 
         "tab_hover_start":{
           "type": "TabControl",
           "label": "Hover"
         },
           "shopengine_catagroy_hover_color":{
             "control": {
               "field": "ColorPalette",
               "label": "Hover Color",
               "props": {
                "clearable": true,
                 "colors": [
                   {
                       "name": "#000",
                       "color": "#000"
                   },
                   {
                       "name": "white",
                       "color": "#fff"
                   },
                   {
                       "name": "blue",
                       "color": "#3400F8"
                   },
                   {
                       "name": "#FE317D",
                       "color": "#FE317D"
                   },
                   {
                       "name": "#d423d4",
                       "color": "#d423d4"
                   }
                   ]
               }
             },
             "responsive": false,
             "type": "object",
             "default": {
               "desktop": "red"
             }
           },
 
         "tab_hover_end":{
           "type": "TabControl"
         },
 
 
       "tab_catagroy_end":{
         "type": "TabContainer"
       },


    "shopengine_typography_start":{
        "type" : "PopoverControl",
        "label" : "Global Typography"
    },

         "shopengine_category_font_family": {
            "control": {
               "field": "FontfamilyControl",
               "label": "Font Family"
            },
            "type": "object"
         },

        "shopengine_category_font_size":{
            "control": {
            "field": "RangeControl",
            "props": {
              "label": "Font Size",
                "min": 0,
                "max": 100,
                "allowReset": true,
                "step": 1
            }
            },
            "responsive": true,
            "type": "object",
            "default": {
            "desktop": 14
            }
        },

        "shopengine_category_font_weight":{
            "control": {
              "field": "SelectControl",
              "label": "Font Weight",
              "props": {
                "options": [
                  {
                    "label": 100,
                    "value": 100
                  },
                  {
                    "label": 200,
                    "value": 200
                  },
                  {
                    "label": 300,
                    "value": 300
                  },
                  {
                    "label": 400,
                    "value": 400
                  },
                  {
                    "label": 500,
                    "value": 500
                  },
                  {
                    "label": 600,
                    "value": 600
                  },
                  {
                    "label": 700,
                    "value": 700
                  },
                  {
                    "label": 800,
                    "value": 800
                  },
                  {
                    "label": 900,
                    "value": 900
                  },
                  {
                    "label": "Bold",
                    "value": "bold"
                  },
                  {
                    "label": "Normal",
                    "value": "normal"
                  }
                ]
              }
            },
            "responsive": false,
            "type": "object",
            "default": {
              "desktop": 500
            }
          },
        
          "shopengine_category_font_transform":{
            "control": {
              "field": "SelectControl",
              "props": {
                "label": "Transform",
                "options": [
                  {
                    "label": "Capitalize",
                    "value": "capitalize"
                  },
                  {
                    "label": "Lowercase",
                    "value": "lowercase"
                  },
                  {
                    "label": "Uppercase",
                    "value": "uppercase"
                  },
                  {
                    "label": "None",
                    "value": "none"
                  }
                ]
              }
            },
            "responsive": false,
            "type": "object",
            "default": {
              "desktop": "capitalize"
            }
          },

          "shopengine_category_font_style":{
            "control": {
              "field": "SelectControl",
              "props": {
                "label": "Style",
                "options": [
                  {
                    "label": "Normal",
                    "value": "normal"
                  },
                  {
                    "label": "Italic",
                    "value": "italic"
                  },
                  {
                    "label": "Oblique",
                    "value": "oblique"
                  },
                  {
                    "label": "Default",
                    "value": "default"
                  }
                ]
              }
            },
            "responsive": false,
            "type": "object",
            "default": {
              "desktop": "normal"
            }
          },

          "shopengine_category_font_Line_height":{
            "control": {
            "field": "RangeControl",
            "label": "Line-Height",
            "props": {
                "min": 0,
                "max": 100,
                "allowReset": true,
                "step": 0.1
            }
            },
            "responsive": true,
            "type": "object",
            "default": {
            "desktop": 17
            }
         },

         "shopengine_category_font_letter_spacing":{
            "control": {
              "field": "RangeControl",
              "props": {
                "label": "Letter Spacing",
                "min": 0,
                "max": 100,
                "allowReset": true,
                "step": 0.1
              }
            },
            "responsive": false,
            "type": "object",
            "default": {
              "desktop": 0
            }
          },

          "shopengine_category_font_word_spacing":{
            "control": {
              "field": "RangeControl",
              "props": {
                "label": "Word Spacing",
                "min": 0,
                "max": 100,
                "allowReset": true,
                "step": 0.1
              }
            },
            "responsive": true,
            "type": "object",
            "default": {
              "desktop": 0,
              "tablet" : 0,
              "mobile": 0
            }
          },
    "shopengine_typography_end":{
        "type" : "PopoverControl"
    },
    "shopengine_section_product_cats_end":{
      "type": "PanelBody",
      "label": "Categories"
  }
}