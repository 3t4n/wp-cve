                    {
                        "label": "Center",
                        "value": "center"
                    },
                    {
                        "label": "Right",
                        "value": "right"
                    }
                ]
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": "left",
            "tablet": "left",
            "mobile": "left"
        }
      },
      "shopengine_thank_you_2_section_divider": {
