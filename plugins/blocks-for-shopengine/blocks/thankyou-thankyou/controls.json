         "type": "object",
         "default": {
               "desktop": "Title"
         }
      },
      "shopengine_order_thankyou_title_color": {
         "control": {
             "field": "ColorPalette",
             "label": "Color",
             "props": {
                 "clearable": true,
                 "colors": [
