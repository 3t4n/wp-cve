    "shopengine_form_container_panel_start":{
        "type":"PanelBody",
        "label":"Form Container"
    },
        "shopengine_form_container_color":{
            "control":{
                "field": "ColorPalette",
                "label": "Container Background",
                "props": {
                    "colors": [
                        { "name": "red", "color": "#979797" },
                        { "name": "black", "color": "#3A3A3A" },
                        { "name": "blue", "color": "#F7F8FB" }
                    ]
                }
            },
            "type": "object",
            "default": {
                "desktop": "#F7F8FB"
            }
        },
        "shopengine_form_container_alignment":{
            "control":{
                "field": "SelectControl", 
                "props": {
                    "label": "Alignment",
                    "options":[
                        { "label": "Left", "value": "left" },
                        { "label": "Right", "value": "right" }
                    ]
                }
            },
            "responsive": true,
            "type": "object",
            "default": {
                "desktop": ""
            }
        },
        "shopengine_form_container_padding":{
            "control":{
                "field": "__experimentalBoxControl",
                "props":{
                    "label": "Padding"
                }
            },
            "responsive": true,
            "type": "object",
            "default": {
                "desktop": {
                    "top":"25px",
                    "right":"30px",
                    "bottom":"25px",
                    "left":"30px"
                }
            }
        },
    "shopengine_form_container_panel_end":{
        "type":"PanelBody"
    },

