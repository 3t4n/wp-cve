    "shopengine_avatar_wrapper_style_section_start":{
        "type":"PanelBody",
        "label":"Wrapper"
    },
    "shopengine_avatar_content_alignmnet": {
        "control": {
            "field": "Choose",
            "props": {
                "label": "Alignment",
                "options": [
                    {
                        "label": "Flex Start",
                        "value": "flex-start",
                        "icon": "dashicons-editor-alignleft"
                    },
                    {
                        "label": "Center",
                        "value": "center",
                        "icon": "dashicons-editor-aligncenter"
                    },
                    {
                        "label": "Flex End",
                        "value": "flex-end",
                        "icon": "dashicons-editor-alignright"
                    },
                    {
                        "label": "Space Between",
                        "value": "space-between",
                        "icon": "dashicons-image-flip-horizontal"
                    },
                    {
                        "label": "Space Around",
                        "value": "space-around",
                        "icon": "dashicons-editor-justify"
                    },
                    {
                        "label": "Space Evenly",
                        "value": "space-evenly",
                        "icon": "dashicons-leftright"
                    }
                ]
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": "flex-start"
        }
    },
    "shopengine_avatar_content_gap": {
        "control": {
          "field": "RangeControl",
          "props": {
              "label": "Content Gap",
              "min": 1,
              "max": 100,
              "step": 1
          }
        },
        "type": "object",
        "default": {
          "desktop": ""
        }
      },

    "shopengine_avatar_wrapper_style_section_end":{
