    "shopengine_category_list": {
        "control": {
            "field": "Ajaxselect2Control",
            "label": "Select Category",
            "props": {
                "options": "ajaxselect2/product_cat",
                "isMulti": true
            }
        },
        "type": "object",
        "default": {
            "desktop": []
        }
    },
    
    "shopengine_show_category_image": {
        "control": {
            "field": "ToggleControl",
