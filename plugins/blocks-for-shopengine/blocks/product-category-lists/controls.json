{
    "settings_section_start": {
        "type": "PanelBody",
        "label": "Settings"
    },
    "shopengine_product_cat_list_styles": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Category Styles",
                "options": [
                    {
                        "label": "Normal",
                        "value": "normal"
                    },
                    {
                        "label": "Style2",
                        "value": "style_2"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": "normal"
        }
    },

    "shopengine_grid_columns": {
        "control": {
            "field": "__experimentalNumberControl",
            "props": {
                "min": 1,
                "max": 12,
                "label": "Column"
            }
        },
        "type": "object",
        "responsive": true,
        "default": {
            "desktop": 3,
            "tablet": 3,
            "mobile": 2
        }
    },

    "settings_section_end": {
        "type": "PanelBody"
    },

