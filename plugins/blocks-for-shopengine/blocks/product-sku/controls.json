{
    "shopengine_section_product_sku_start": {
        "type": "PanelBody",
        "label": "SKU"
    },
    "shopengine_product_sku_align": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Alignment",
                "options": [
                    {
                        "label": "Left",
                        "value": "left"
                    },
                    {
                        "label": "Center",
                        "value": "center"
                    },
                    {
                        "label": "Right",
                        "value": "right"
                    }
                ]
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": "left"
        }
    },
    "shopengine_heading_product_sku_separator": {
        "control": {
            "field": "__experimentalDivider"
        },
        "type": "object"
    },
    "shopengine_heading_product_sku_label": {
        "control": {
            "field": "__experimentalHeading",
            "props": {
                "children": "SKU Label"
            }
        },
        "type": "object"
    },
    "shopengine_product_sku_label_show": {
        "control": {
            "field": "ToggleControl",
            "props": {
                "label": "Show Label",
                "checked": true
            }
        },
        "type": "object",
        "default": {
            "desktop": true
        }
    },
    "shopengine_product_sku_label_color": {
        "control": {
            "field": "ColorPalette",
            "label": "Label color",
            "props": {
                "colors": [
                    {
                        "name": "red",
                        "color": "#f00"
                    },
                    {
                        "name": "white",
                        "color": "#fff"
                    },
                    {
                        "name": "blue",
                        "color": "#00f"
                    },
                    {
                        "name": "black",
                        "color": "#101010"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": "#101010"
        }
    },
    "shopengine_product_cats_label_text_decoration": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Text Decoration",
                "options": [
                    {
                        "label": "None",
                        "value": "none"
                    },
                    {
                        "label": "Underline",
                        "value": "underline"
                    },
                    {
                        "label": "Overline",
                        "value": "overline"
                    },
                    {
                        "label": "Line Through",
                        "value": "line-through"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": "none"
        }
    },
    "shopengine_heading_product_sku_value_separator": {
        "control": {
            "field": "__experimentalDivider"
        },
        "type": "object"
    },
    "shopengine_heading_product_sku_value": {
        "control": {
            "field": "__experimentalHeading",
            "props": {
                "children": "SKU Value"
            }
        },
        "type": "object"
    },
    "shopengine_product_sku_value_color": {
        "control": {
            "field": "ColorPalette",
            "label": "Value color",
            "props": {
                "colors": [
                    {
                        "name": "red",
                        "color": "#f00"
                    },
                    {
                        "name": "white",
                        "color": "#fff"
                    },
                    {
                        "name": "blue",
                        "color": "#00f"
                    },
                    {
                        "name": "black",
                        "color": "#101010"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": "#A0A0A0"
        }
    },
    "shopengine_product_sku_typography_separator": {
        "control": {
            "field": "__experimentalDivider"
        },
        "type": "object"
    },
    "shopengine_product_sku_typography_heading": {
        "control": {
            "field": "__experimentalHeading",
            "props": {
                "children": "Global Typography"
            }
        },
        "type": "object"
    },
    "shopengine_product_sku_typography_start": {
        "type": "PopoverControl",
        "label": "Typography"
    },
    "shopengine_font_family": {
        "control": {
            "field": "FontfamilyControl",
            "label": "Font Family"
        },
        "type": "object"
    },
    "shopengine_font_size": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Font Size(px)",
                "min": 0,
                "max": 100,
                "step": 1
            }
        },
        "responsive": false,
        "type": "object",
        "default": {
            "desktop": 14
        }
    },
    "shopengine_font_weight": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Font Weight",
                "options": [
                    {
                        "label": "100",
                        "value": "100"
                    },
                    {
                        "label": "200",
                        "value": "200"
                    },
                    {
                        "label": "300",
                        "value": "300"
                    },
                    {
                        "label": "400",
                        "value": "400"
                    },
                    {
                        "label": "500",
                        "value": "500"
                    },
                    {
                        "label": "600",
                        "value": "600"
                    },
                    {
                        "label": "700",
                        "value": "700"
                    },
                    {
                        "label": "800",
                        "value": "800"
                    },
                    {
                        "label": "900",
                        "value": "900"
                    },
                    {
                        "label": "Default",
                        "value": "normal"
                    },
                    {
                        "label": "Normal",
                        "value": "normal"
                    },
                    {
                        "label": "Bold",
                        "value": "bold"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": 500
        }
    },
    "shopengine_text_transform": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Text Transform",
                "options": [
                    {
                        "label": "Default",
                        "value": "default"
                    },
                    {
                        "label": "Uppercase",
                        "value": "uppercase"
                    },
                    {
                        "label": "Lowercase",
                        "value": "lowercase"
                    },
                    {
                        "label": "Capitalize",
                        "value": "capitalize"
                    },
                    {
                        "label": "None",
                        "value": "none"
                    }
                ]
            }
        },
        "responsive": false,
        "type": "object",
        "default": {
            "desktop": "none"
        }
    },
    "shopengine_line_height": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Line Height(PX)",
                "min": 0,
                "max": 100,
                "step": 1
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": 17
        }
    },
    "shopengine_word_spacing": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Word Spacing(em)",
                "min": 0,
                "max": 100,
                "step": 1
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
            "desktop": ""
        }
    },
    "shopengine_product_sku_typography_end": {
        "type": "PopoverControl"
    },
    "shopengine_section_product_sku_end": {
        "type": "PanelBody",
        "label": "SKU"
    }
}