{
  "shopengine_section_filter_orderby_start": {
    "type": "PanelBody",
    "label": "Order By Filter"
  },
  "shopengine_orderby_type": {
    "control": {
      "field": "SelectControl",
      "props": {
        "label": "Order By Filter Type",
        "options": [
          {
            "label": "Dropdown",
            "value": "dropdown"
          },
          {
            "label": "List",
            "value": "list"
          }
        ]
      }
    },
    "type": "object",
    "default": {
      "desktop": "dropdown"
    }
  },
  "shopengine_orderby_height": {
    "control": {
      "field": "RangeControl",
      "props": {
        "label": "Height(PX)",
        "min": 0,
        "max": 100,
        "step": 1
      }
    },
    "type": "object",
    "default": {
      "desktop": 40
    }
  },

  "shopengine_orderby_filter_typography_start": {
