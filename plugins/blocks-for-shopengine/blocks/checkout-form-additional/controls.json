{
    "shopengine_Heading_panel_start": {
        "type": "PanelBody",
        "label": "Info"
    },
    "shopengine_heading_show": {
        "control": {
            "field": "ToggleControl",
            "props":{
                "label": "Show Heading",
                "checked": true
            }
        },
        "type": "object",
        "default": {
            "desktop": false
        }
    },
    "shopengine_heading_color": {
        "control": {
            "field": "ColorPalette",
            "label": "Color",
            "props": {
                "colors": [
                    { "name": "red", "color": "#979797" },
                    { "name": "black", "color": "#3A3A3A" },
                    { "name": "blue", "color": "#00f" }
                ]
            }
        },
        
        "type": "object",
        "default": {
            "desktop": ""
        }
    },
    "shopengine_heading_font_size": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Font Size(px)",
                "min": 0,
                "max": 100
            }
        },
        
        "type": "object",
        "default": {
            "desktop": ""
        }
    },
    "shopengine_heading_spacing_bottom": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Spacing Bottom(px)",
                "min": 0,
                "max": 100
            }
        },
        "type": "object",
        "default": {
            "desktop": ""
        }
    },
    "shopengine_Heading_panel_end": {
        "type": "PanelBody"
    },

