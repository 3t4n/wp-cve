                    {
                        "label": "Default",
                        "value": "normal"
                    },
                    {
                        "label": "Normal",
                        "value": "normal"
                    },
                    {
                        "label": "Bold",
                        "value": "bold"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": 700
        }
    },
    "shopengine_title_transform": {
        "control": {
            "field": "SelectControl",
            "props": {
                "label": "Transform",
                "options": [
                    {
                        "label": "Default",
