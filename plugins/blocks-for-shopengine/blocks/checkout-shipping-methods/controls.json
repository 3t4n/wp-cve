                    {
                        "label": "Normal",
                        "value": "normal"
                    }
                ]
            }
        },
        "type": "object",
        "default": {
            "desktop": ""
        }
    },

    "shopengine_title_wordspace": {
        "control": {
            "field": "RangeControl",
            "props": {
                "label": "Word Spacing(px)",
                "min": 0,
                "max": 100,
                "step": 1
            }
        },
        "responsive": true,
        "type": "object",
        "default": {
