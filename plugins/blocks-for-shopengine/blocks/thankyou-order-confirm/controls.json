            "responsive": false,
            "type": "object",
            "default": {
                "desktop": "capitalize",
                "tablet": "capitalize",
                "mobile": "capitalize"
            }
        },
        "shopengine_thankyou_order_confirm_line_height": {
            "control": {
                "field": "__experimentalUnitControl",
