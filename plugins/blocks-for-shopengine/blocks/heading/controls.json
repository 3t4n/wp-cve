{
    "shopengine_heading_panel_start":{
        "type":"PanelBody",
        "label":"Title"
    },
     "shopengine_title_heading_input":{
        "control":{
           "field": "TextareaControl",
           "props":{
            "label": "Title"
           }
        },
        "responsive": false,
         "type": "object",
         "default": {
            "desktop": "Shopengine Title"
         }
     },

      "shopengine_title_header_size": {
         "control": {
            "field": "SelectControl",
            "label": "HTML Tag",
            "props": {
               "options": [
                     {
                        "label": "H1",
                        "value": "h1"
                     },
                     {
                        "label": "H2",
                        "value": "h2"
                     },
                     {
                        "label": "H3",
                        "value": "h3"
                     },
                     {
                        "label": "H4",
                        "value": "h4"
                     },
                     {
                        "label": "H5",
                        "value": "h5"
                     },
                     {
                        "label": "H6",
                        "value": "h6"
                     },
                     {
                        "label": "Div",
                        "value": "div"
                     },
                     {
                        "label": "Span",
                        "value": "span"
                     },
                     {
                        "label": "P",
                        "value": "p"
                     }
               ]
            }
         },
         "responsive": false,
         "type": "object",
         "default": {
            "desktop": "h4"
         }
   },
   "shopengine_heading_title_align": {
         "control": {
            "field": "Choose",
            "props": {
               "label": "Alignment",
               "options": [
                  {
                     "value": "left",
                     "label": "Left",
                     "icon": "dashicons-editor-alignleft"
                  },
                  {
                     "value": "center",
                     "label": "Center",
                     "icon": "dashicons-editor-aligncenter"
                  },
                  {
                     "value": "right",
                     "label": "Right",
                     "icon": "dashicons-editor-alignright"
                  }
               ]
            }
         },
         "responsive": true,
         "type": "object",
         "default": {
            "desktop": "left"
         }
   },
  "shopengine_heading_panel_end":{
      "type":"PanelBody"
  },

   "shopengine_heading_style_panel_start":{
      "type":"PanelBody",
      "label":"Title Style"
   },

   "shopengine_heading_title_color": {
         "control": {
            "field": "ColorControl",
            "props": {
               "label": "Text Color",
               "alpha": false
            }
         },
         "responsive": false,
         "type": "object",
         "default": {
            "desktop": ""
         }
   },
   "shopengine_heading_title_typography_start": {
      "type": "PopoverControl",
      "label": "Text Typography"
   },

   "shopengine_heading_title_font_family": {
      "control": {
          "field": "FontfamilyControl",
          "label": "Font Family"
      },
      "type": "object"
  },
  "shopengine_heading_title_font_size": {
      "control": {
          "field": "RangeControl",
          "props": {
             "label": "Font Size(px)",
             "withInputField": true,
              "allowReset": true,
              "min": 0,
              "max": 200,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": ""
      }
  },
  "shopengine_heading_title_font_weight": {
      "control": {
          "field": "SelectControl",
          "props": {
             "label": "Font Weight",
              "options": [{
                      "label": "100",
                      "value": "100"
                  },
                  {
                      "label": "200",
                      "value": "200"
                  },
                  {
                      "label": "300",
                      "value": "300"
                  },
                  {
                      "label": "400",
                      "value": "400"
                  },
                  {
                      "label": "500",
                      "value": "500"
                  },
                  {
                      "label": "600",
                      "value": "600"
                  },
                  {
                      "label": "700",
                      "value": "700"
                  },
                  {
                      "label": "800",
                      "value": "800"
                  },
                  {
                      "label": "900",
                      "value": "900"
                  }
              ]
          }
      },
      "responsive": false,
      "type": "object",
      "default": {
          "desktop": 600
      }
  },
  "shopengine_heading_title_font_style": {
      "control": {
          "field": "SelectControl",
          "props": {
             "label": "Style",
              "options": [{
                      "label": "Default",
                      "value": ""
                  },
                  {
                      "label": "Normal",
                      "value": "normal"
                  },
                  {
                      "label": "Italic",
                      "value": "italic"
                  },
                  {
                      "label": "Oblique",
                      "value": "oblique"
                  }
              ]
          }
      },
      "responsive": false,
      "type": "object",
      "default": {
          "desktop": "normal"
      }
  },
  "shopengine_heading_title_text_transform": {
      "control": {
          "field": "SelectControl",
          "props": {
             "label": "Transform",
              "options": [{
                      "label": "Default",
                      "value": ""
                  },
                  {
                      "label": "Uppercase",
                      "value": "uppercase"
                  },
                  {
                      "label": "Lowercase",
                      "value": "lowercase"
                  },
                  {
                      "label": "Capitalize",
                      "value": "capitalize"
                  }
              ]
          }
      },
      "responsive": false,
      "type": "object",
      "default": {
          "desktop": ""
      }
  },
  "shopengine_heading_title_line_height": {
      "control": {
          "field": "RangeControl",
          "props": {
             "label": "Line Height(px)",
             "allowReset": true,
              "min": 0,
              "max": 100,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": 24
      }
  },
  "shopengine_heading_title_letter_spacing": {
      "control": {
          "field": "RangeControl",
          "props": {
             "label": "Letter Spacing(px)",
              "allowReset": true,
              "min": 0,
              "max": 100,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": 0.1
      }
  },
  "shopengine_heading_title_wordspace": {
      "control": {
          "field": "RangeControl",
          "props": {
             "label": "Word Spacing(px)",
              "allowReset": true,
              "min": 0,
              "max": 100,
              "step": 1
          }
      },
      "responsive": true,
      "type": "object",
      "default": {
          "desktop": ""
      }
  },
  "shopengine_heading_title_typography_end": {
      "type": "PopoverControl"
  },

   "shopengine_heading_style_panel_end":{
      "type":"PanelBody"
      
   }
}