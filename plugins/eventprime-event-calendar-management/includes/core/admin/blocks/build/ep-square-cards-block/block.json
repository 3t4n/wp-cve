{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/ep-square-cards-block",
  "version": "0.1.0",
  "title": "Event Prime Events Listing",
  "category": "event-prime",
  "icon": "smiley",
  "description": "Event Prime Event Listing block",
  "providesContext": {
    "create-block/block_square_card_columns": "block_square_card_columns"
  },
  "attributes": {
    "block_square_card_custom_class": {
      "type": "string",
      "default": "block_square_card_custom_class"
    },
    "block_square_card_columns": {
      "type": "number",
      "default": 3
    },
    "block_square_disable_load_more_button": {
      "type": "boolean",
      "default": true
    },
    "block_square_card_load_more_button": {
      "type": "string",
      "default": " Load More "
    },
    "block_square_card_fetch_events": {
      "type": "number",
      "default": 10
    },
    "ep_block_all_views": {
      "type": "string",
      "default": "square"
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "ep-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}