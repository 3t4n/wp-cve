msgstr "Käyttäjänimi"

msgid "User Email"
msgstr "Sähköposti"

msgid "Password"
msgstr "Salasana"

msgid "Retype Password"
msgstr "Toista salasana"

msgid "First Name"
msgstr "Etunimi"

msgid "Last Name"
msgstr "Sukunimi"

