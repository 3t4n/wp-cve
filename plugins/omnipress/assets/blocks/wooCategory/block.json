{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "omnipress/woocategory",
  "version": "1.0.0",
  "title": "Products Category",
  "category": "omnipress",
  "icon": "",
  "description": "Woocommerce Product Category -- Display a list of products from WooCommerce product category with a chosen preset style, offering flexibility in how it looks with different settings.",
  "supports": {
    "html": false
  },
  "attributes": {
    "preset": {
      "type": "string",
      "default": "one"
    },
    "columns": {
      "type": "number",
      "default": 6
    },
    "subCategory": {
      "type": "boolean",
      "default": true
    },
    "rows": {
      "type": "number",
      "default": 1
    },
    "carousel": {
      "type": "boolean",
      "default": false
    },
    "options": {
      "type": "string",
      "default": "arrow pagination"
    },
    "arrowNext": {
      "type": "string",
      "default": "fa fa-angle-right"
    },
    "arrowPrev": {
      "type": "string",
      "default": "fa fa-angle-left"
    }
  },
  "textdomain": "productcategory",
  "viewScript": "file:./view.js",
  "render": "file:./render.php",
  "style": [
    "file:./index.css"
  ]
}