        },
        "bg": {
            "type": "boolean",
            "default": true
        },
        "barBg": {
            "type": "boolean",
            "default": true
        },
        "valuePos": {
            "type": "string",
            "default": "right"
        },
        "labelPos": {
            "type": "string",
            "default": "top"
        }
