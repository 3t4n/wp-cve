{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gutena/instagram-gallery",
  "title": "Instagram Gallery",
  "category": "gutena",
  "keywords": [
    "instagram",
    "instagram feed",
    "instagram gallery",
    "instagram photos",
    "instagram widget"
  ],
  "description": "Instagram Gallery for Block Editor by Gutena",
  "textdomain": "photofeed-block-gutena",
  "attributes": {
    "instagramToken": {
      "type": "string",
      "default": ""
    },
    "columns": {
      "type": "number",
      "default": 3,
      "minimum": 1,
      "maximum": 6
    },
    "count": {
      "type": "number",
      "default": 6,
      "minimum": 3
    },
    "gridGap": {
      "type": "string",
      "default": "1.5rem"
    },
    "hoverColor": {
      "type": "string",
      "default": "#0006"
    },
    "hoverEffect": {
      "type": "string",
      "default": "none"
    },
    "opacity": {
      "type": "number",
      "default": 1
    },
    "linkType": {
      "type": "string",
      "default": "instagram"
    },
    "linkTarget": {
      "type": "string",
      "default": "_blank"
    },
    "showCaption": {
      "type": "boolean",
      "default": true
    },
    "captionColor": {
      "type": "string",
      "default": "#3306EF"
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}