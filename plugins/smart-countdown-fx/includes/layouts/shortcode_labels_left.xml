<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<paddings>
		<years>1</years>
		<months>2</months>
		<weeks>1</weeks>
		<days>2</days>
		<hours>2</hours>
		<minutes>2</minutes>
		<seconds>2</seconds>
	</paddings>
	<layout>horz</layout>
	<event_text_pos>vert</event_text_pos>
	<labels_pos>left</labels_pos>
	<labels_vert_align>middle</labels_vert_align>
	<hide_highest_zeros>1</hide_highest_zeros>
	<allow_all_zeros>0</allow_all_zeros>
	<responsive value="1">
		<scale value="0.3">
			<layout>vert</layout>
			<labels_pos>col</labels_pos>
		</scale>
		<scale value="0.5">
			<layout>vert</layout>
			<labels_pos>row</labels_pos>
		</scale>
		<scale value="0.8">
			<layout>horz</layout>
			<labels_pos>col</labels_pos>
		</scale>
	</responsive>
</layout>