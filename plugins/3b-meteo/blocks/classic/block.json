  "keywords": [
	"meteo",
	"meteo italia",
	"previsioni del tempo",
	"meteo sul sito",
	"previsioni meteo",
	"3b meteo",
	"block meteo"
  ],
  "icon": "cloud",
  "description": "E' la versione di widget attualmente disponibile sul sito all'indirizzo https://www.3bmeteo.com/widgets",
  "attributes": {
	"tm": {
	  "type": "string",
	  "default": "localita_1_giorno_compatto"
