	"minHeight": {
	  "type": "string",
	  "default": "250px"
	},
	"colorfield1": {
	  "type": "string",
	  "default": "#FC9B2A"
	},
	"colorfield2": {
	  "type": "string",
	  "default": "#FFFFFF"
	},
	"colorfield3": {
	  "type": "string",
	  "default": "#FFFFFF"
	},
	"colorfield4": {
	  "type": "string",
	  "default": "#5E5E5E"
	},
	"lang": {
	  "type": "string",
	  "default": "it"
	},
	"selectedLocality": {
	  "type": "string",
	  "default": ""
