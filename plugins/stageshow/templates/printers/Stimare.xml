<?xml version="1.0" encoding="utf-8"?>
<StageShowPrinter Version="8.0" Units="twips">
	<Id>Stimare EscPos Printer</Id>
	<Protocol>EscPos</Protocol>
	<Codes>
		<Mode>Cut All Tickets</Mode>
		<TicketTerminator>1D F8 1D 56 00 1C C1 18</TicketTerminator>
		<GroupTerminator>1D F8 1D 56 00 1C C1 18</GroupTerminator>
	</Codes>
	<Codes>
		<Mode>Cut at End</Mode>
		<TicketTerminator>1D F8 1C C1 18</TicketTerminator>
		<GroupTerminator>1D F8 1D 56 00 1C C1 18</GroupTerminator>
	</Codes>
	<Notes>
		1D F8 - Ticket Align at Cut
		1D 56 00 - Cut at current position
		1C C1 - Paper Recovery after cut
		18 - Go to start of line
	</Notes>
</StageShowPrinter>
