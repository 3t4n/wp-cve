{
	"apiVersion": "2",
	"name": "pb/accordion-item",
	"title": "Accordion Item",
	"category": "text",
	"textdomain": "accordion-blocks",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": ".c-accordion__title"
		},
		"initiallyOpen": {
			"type": "boolean",
			"default": false
		},
		"clickToClose": {
			"type": "boolean",
			"default": true
		},
		"autoClose": {
			"type": "boolean",
			"default": true
		},
		"titleTag": {
			"type": "string",
			"default": "h2"
		},
		"scroll": {
			"type": "boolean",
			"default": false
		},
		"scrollOffset": {
			"type": "number",
			"default": 0
		},
		"uuid": {
			"type": "number"
		}
	},
	"supports": {
		"anchor": true
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css"
}
