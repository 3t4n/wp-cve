{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "dotdigital-for-wordpress/signup-form",
  "title": "Dotdigital Signup Form",
  "category": "widgets",
  "description": "Add a signup form.",
  "supports": {
    "html": true,
    "anchor": true
  },
  "textdomain": "dotdigital-for-wordpress",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "attributes": {
    "showtitle": {
      "type": "boolean",
      "default": true
    },
    "showdesc": {
      "type": "boolean",
      "default": true
    },
    "redirecturl": {
      "type": "string"
    },
    "is_ajax": {
      "type": "boolean",
      "default": false
    }
  },
  "keywords": [
    "form"
  ]
}