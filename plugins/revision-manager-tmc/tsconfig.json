{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": false,
        "strictNullChecks": false,
        "jsx": "preserve",
        "importHelpers": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "noUnusedParameters": false,
        "noUnusedLocals": false
    }
}
