msgstr "esimene"

#: wp-conditional-captcha.php:417
msgid "second"
msgstr "teine"

#: wp-conditional-captcha.php:417
msgid "third"
msgstr "kolmas"

#: wp-conditional-captcha.php:417
msgid "fourth"
msgstr "neljas"

#: wp-conditional-captcha.php:417
msgid "fifth"
msgstr "viies"

#: wp-conditional-captcha.php:417
msgid "sixth"
msgstr "kuues"

#: wp-conditional-captcha.php:419
msgid "What are the %1$s and %2$s characters of the following sequence?"
msgstr "Millised on %1$s ja %2$s tegelased  järjekorras?"

#: wp-conditional-captcha.php:433
msgid "Sorry, the CAPTCHA wasn't entered correctly. (reCAPTCHA said: %s)"
msgstr "Vabandame, CAPTCHA ei olnud õiged. (reCAPTCHA ütles: %s)"

#: wp-conditional-captcha.php:437
msgid "Sorry, the CAPTCHA wasn't entered correctly."
msgstr "Vabandame, CAPTCHA ei olnud õiged."

