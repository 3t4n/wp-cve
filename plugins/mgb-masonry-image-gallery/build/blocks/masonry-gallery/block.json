{
  "apiVersion": 2,
  "name": "migb/masonry-gallery",
  "version": "0.0.0",
  "title": "Masonry Gallery",
  "category": "media",
  "description": "Showcase Images in a Masonry Gallery View.",
  "keywords": [
    "Masonry",
    "Gallery",
    "Images",
    "Image Gallery"
  ],
  "supports": {
    "html": false,
    "anchor": false,
    "customClassName": false
  },
  "attributes": {
    "galleryId": {
      "type": "string"
    },
    "images": {
      "type": "array"
    },
    "deskCol": {
      "type": "number",
      "default": 3
    },
    "tabCol": {
      "type": "number",
      "default": 2
    },
    "phoneCol": {
      "type": "number",
      "default": 1
    },
    "deskGap": {
      "type": "number",
      "default": 10
    },
    "tabGap": {
      "type": "number",
      "default": 10
    },
    "phoneGap": {
      "type": "number",
      "default": 5
    },
    "enableLightbox": {
      "type": "boolean",
      "default": true
    },
    "imageHoverEffect": {
      "type": "string",
      "default": "none"
    },
    "photoBorderStyle": {
      "type": "string",
      "default": "solid"
    },
    "photoBorderColor": {
      "type": "string"
    },
    "photoTopBorderWidth": {
      "type": "string",
      "default": "0"
    },
    "photoRightBorderWidth": {
      "type": "string",
      "default": "0"
    },
    "photoBottomBorderWidth": {
      "type": "string",
      "default": "0"
    },
    "photoLeftBorderWidth": {
      "type": "string",
      "default": "0"
    },
    "enablePhotoLinkedBorder": {
      "type": "boolean",
      "default": true
    },
    "photoLinkedBorderWidth": {
      "type": "string",
      "default": "0"
    },
    "photoTopBorderRadius": {
      "type": "string",
      "default": "0"
    },
    "photoRightBorderRadius": {
      "type": "string",
      "default": "0"
    },
    "photoBottomBorderRadius": {
      "type": "string",
      "default": "0"
    },
    "photoLeftBorderRadius": {
      "type": "string",
      "default": "0"
    },
    "enablePhotoLinkedBorderRadius": {
      "type": "boolean",
      "default": true
    },
    "photoLinkedBorderRadius": {
      "type": "string",
      "default": "0"
    },
    "showCaption": {
      "type": "boolean",
      "default": false
    },
    "captionColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "captionPosition": {
      "type": "string",
      "default": "center"
    },
    "captionDeskFontSize": {
      "type": "number",
      "default": 16
    },
    "captionTabFontSize": {
      "type": "number",
      "default": 15
    },
    "captionPhoneFontSize": {
      "type": "number",
      "default": 14
    },
    "enableCustomColor": {
      "type": "boolean",
      "default": false
    },
    "zIndex": {
      "type": "number"
    },
    "customClass": {
      "type": "string"
    }
  },
  "textdomain": "masonry-image-gallery",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}