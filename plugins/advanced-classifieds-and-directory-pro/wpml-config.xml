        <custom-field action="copy-once">listing_duration</custom-field> 
        <custom-field action="copy-once">is_acadp_subscription</custom-field> 
        <custom-field action="copy-once">acadp_listings_limit</custom-field> 
        <custom-field action="copy-once">acadp_listing_duration</custom-field> 
        <custom-field action="copy-once">acadp_images_limit</custom-field> 
        <custom-field action="copy-once">acadp_featured</custom-field> 
        <custom-field action="copy-once">acadp_categories</custom-field> 
        <custom-field action="copy-once">acadp_disable_repeat_purchase</custom-field> 
    </custom-fields>
    <custom-term-fields>
        <custom-term-field action="copy-once">image</custom-term-field>
    </custom-term-fields>
    <admin-texts>
        <key name="acadp_badges_settings">
            <key name="new_listing_label"/>
            <key name="popular_listing_label"/>
            <key name="sold_listing_label"/>
        </key>
        <key name="acadp_terms_of_agreement">
            <key name="agree_label"/>
            <key name="agree_text"/>
        </key>
        <key name="acadp_privacy_policy">
            <key name="privacy_policy_label"/>
            <key name="privacy_policy_text"/>
        </key>
        <key name="acadp_cookie_consent">
            <key name="consent_button_label"/>
            <key name="consent_message"/>
        </key>
        <key name="acadp_featured_listing_settings">
            <key name="label"/>
            <key name="description"/>
        </key>
        <key name="acadp_fee_plans_settings">
            <key name="label"/>
            <key name="description"/>
        </key>
        <key name="acadp_gateway_offline_settings">
            <key name="label"/>
