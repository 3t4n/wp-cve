<wpml-config>    
    <custom-fields>
        <custom-field action="copy-once">never_expires</custom-field>
        <custom-field action="copy-once">expiry_date</custom-field>
        <custom-field action="copy-once">featured</custom-field>
        <custom-field action="copy-once">sold</custom-field>
        <custom-field action="copy-once">listing_status</custom-field>
        <custom-field action="copy-once">price</custom-field> 
        <custom-field action="copy-once">views</custom-field> 
        <custom-field action="copy-once">address</custom-field>
        <custom-field action="copy-once">zipcode</custom-field>
        <custom-field action="copy-once">phone</custom-field>
        <custom-field action="copy-once">email</custom-field>
        <custom-field action="copy-once">website</custom-field>
        <custom-field action="copy-once">latitude</custom-field>
        <custom-field action="copy-once">longitude</custom-field>
        <custom-field action="copy-once">hide_map</custom-field> 
