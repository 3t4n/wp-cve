        <custom-field action="copy-once">required</custom-field>
        <custom-field action="copy-once">choices</custom-field> 
        <custom-field action="copy-once">default_value</custom-field>
        <custom-field action="copy-once">min</custom-field> 
        <custom-field action="copy-once">max</custom-field> 
        <custom-field action="copy-once">step</custom-field>
        <custom-field action="copy-once">allow_null</custom-field>  
        <custom-field action="copy-once">placeholder</custom-field> 
        <custom-field action="copy-once">rows</custom-field> 
        <custom-field action="copy-once">target</custom-field> 
        <custom-field action="copy-once">nofollow</custom-field> 
        <custom-field action="copy-once">associate</custom-field> 
