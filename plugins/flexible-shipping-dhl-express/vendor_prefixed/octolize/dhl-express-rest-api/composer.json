{
    "name": "octolize\/dhl-express-rest-api",
    "description": "DHL Express Rest Api",
    "license": "MIT",
    "keywords": [
        "wordpress",
        "woocommerce",
        "dhl",
        "dhl express"
    ],
    "homepage": "https:\/\/gitlab.wpdesk.dev\/wpdesk\/predators\/library\/dhl-express-rest-api",
    "authors": [
        {
            "name": "grola",
            "email": "grola@octolize.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "psr\/log": "^1.1",
        "guzzlehttp\/guzzle": "^7.8",
        "ramsey\/uuid": "^4.2"
    },
    "require-dev": {
        "phpunit\/phpunit": "*",
        "mockery\/mockery": "*",
        "10up\/wp_mock": "*",
        "phpcompatibility\/php-compatibility": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "DhlVendor\\Octolize\\DhlExpress\\": "src\/DhlExpress"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests\/unit"
        ]
    },
    "extra": {
        "text-domain": "dhl-express-rest-api",
        "translations-folder": "lang",
        "po-files": {
            "pl_PL": "pl_PL.po",
            "de_DE": "de_DE.po"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit-unit": "phpunit --configuration phpunit-unit.xml --coverage-text --colors=never",
        "phpunit-unit-fast": "phpunit --configuration phpunit-unit.xml --no-coverage",
        "phpunit-integration": "phpunit --configuration phpunit-integration.xml --coverage-text --colors=never",
        "phpunit-integration-fast": "phpunit --configuration phpunit-integration.xml --no-coverage"
    }
}