	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"start:todo-item": "wp-scripts start blocks/todo-item/src/index.js --output-path=blocks/todo-item/build/",
		"build:todo-item": "wp-scripts build blocks/todo-item/src/index.js --output-path=blocks/todo-item/build/",
		"start:todo-list": "wp-scripts start blocks/todo-list/src/index.js --output-path=blocks/todo-list/build/",
		"build:todo-list": "wp-scripts build blocks/todo-list/src/index.js --output-path=blocks/todo-list/build/",
		"start": "npm run start:todo-item",
		"build": "npm run build:todo-list && npm run build:todo-item",
		"format": "wp-scripts format",
